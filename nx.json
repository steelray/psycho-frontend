{
  "npmScope": "psycho",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "core": {
      "tags": []
    },
    "features": {
      "tags": []
    },
    "scss": {
      "tags": []
    },
    "utils": {
      "tags": []
    },
    "web-core": {
      "tags": []
    },
    "web-features": {
      "tags": []
    },
    "web-scss": {
      "tags": []
    },
    "website": {
      "tags": []
    },
    "website-e2e": {
      "tags": [],
      "implicitDependencies": [
        "website"
      ]
    }
  }
}